@model IList<Actor>
<table class="table table-striped">
    <thead>
        <tr>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (Actor actor in Model)
        {
            <tr>
                <td>@actor.FirstName @actor.LastName</td>
            </tr>
        }
    </tbody>
</table>